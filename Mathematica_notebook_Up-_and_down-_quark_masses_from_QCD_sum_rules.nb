Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "sth", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"sth", "=", "0"}], ";", " ", 
