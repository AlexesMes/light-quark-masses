Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"modelBPdR", "[", 
    RowBox[{
    "s_", ",", "\[Lambda]c_", ",", "width1_", ",", "width2_", ",", 
     "\[Kappa]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "mpi1", ",", "\[CapitalGamma]pi1", " ", ",", "mpi2", ",", 
       "\[CapitalGamma]pi2", " ", ",", "\[Lambda]", ",", "m", ",", 
       "\[CapitalGamma]", ",", "BW", ",", "BW1", ",", "BW2", ",", "P"}], 
      "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"mpi1", "=", " ", 
       FractionBox["1300", "1000"]}], "   ", ";", 
      RowBox[{"\[CapitalGamma]pi1", " ", "=", " ", 
       FractionBox["width1", "1000"]}], ";", 
      RowBox[{"mpi2", " ", "=", " ", 
       FractionBox["1812", "1000"]}], ";", 
      RowBox[{"\[CapitalGamma]pi2", " ", "=", " ", 
       FractionBox["width2", "1000"]}], ";", 
      RowBox[{"\[Lambda]", "=", 
       RowBox[{"\[Kappa]", "*", "\[Lambda]c"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Breit", "-", 
         RowBox[{"Wigner", " ", "model", " ", "normalized", " ", "such", " ", 
          RowBox[{"that", ":", " ", 
           RowBox[{"BW1", 
            RowBox[{"(", "0", ")"}]}]}]}]}], "=", 
        RowBox[{
         RowBox[{"BW2", 
          RowBox[{"(", "0", ")"}]}], "=", "1"}]}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"BW", "=", " ", 
       RowBox[{"(", 
        FractionBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["m", "2"], "-", "sth"}], ")"}], "2"], "+", " ", 
          RowBox[{
           SuperscriptBox["m", "2"], "*", 
           SuperscriptBox["\[CapitalGamma]", "2"]}]}], 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"s", "-", 
             SuperscriptBox["m", "2"]}], ")"}], "2"], "+", " ", 
          RowBox[{
           SuperscriptBox["m", "2"], "*", 
           SuperscriptBox["\[CapitalGamma]", "2"], " "}]}]], ")"}]}], ";", 
      " ", "\[IndentingNewLine]", 
      RowBox[{"BW1", " ", "=", " ", 
       RowBox[{"BW", "/.", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"m", "\[Rule]", " ", "mpi1"}], ",", 
          RowBox[{"\[CapitalGamma]", "\[Rule]", " ", "\[CapitalGamma]pi1"}]}],
          "}"}]}]}], ";", 
      RowBox[{"BW2", "=", " ", 
       RowBox[{"BW", "/.", " ", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"m", "\[Rule]", " ", "mpi2"}], ",", 
          RowBox[{"\[CapitalGamma]", "\[Rule]", " ", "\[CapitalGamma]pi2"}]}],
          "}"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"IPS", "[", 
        RowBox[{"k_", "?", "NumericQ"}], "]"}], ":=", 
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Sqrt", "[", 
