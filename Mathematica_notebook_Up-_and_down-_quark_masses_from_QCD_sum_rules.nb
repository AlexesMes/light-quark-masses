   RowBox[{
    RowBox[{"modelBPdR2", "[", 
     RowBox[{
     "s_", ",", "\[Lambda]c_", ",", "width1_", ",", "width2_", ",", 
      "\[Kappa]_"}], "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
