Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"contourimproved", "[", 
    RowBox[{
    "kernelcontribution_", ",", "correlator_", ",", " ", "step_", ",", 
     "aint_", ",", "sint_", ",", "s0_"}], "]"}], " ", ":=", " ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "h", ",", "rgealpha", ",", " ", "ralpha", ",", " ", "rgemass", ",", " ",
        "rmass", ",", " ", "contourintegral"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"h", "=", 
       RowBox[{"Pi", "/", "step"}]}], ";", "\n", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Solving", " ", 
         SubscriptBox["\[Alpha]", "s"], 
         RowBox[{"(", "s", ")"}], " ", "renormalization", " ", "group", " ", 
         "differential", " ", "equation"}], ",", " ", 
        RowBox[{"between", " ", "-", 
         RowBox[{"\[Pi]", " ", "and", " ", 
          RowBox[{"\[Pi]", ".", " ", "The"}], " ", "output", " ", "is", " ", 
          "the", " ", "running", " ", "coupling", " ", "constant", " ", 
          "given", " ", "\n", "as", " ", "an", " ", "interpolation", " ", 
          "function", " ", "over", " ", "the", " ", "points", " ", "q"}]}], 
        ",", " ", 
        RowBox[{
         RowBox[{
         "where", " ", "q", " ", "ranges", " ", "linearly", " ", "between"}], 
         " ", "-", 
         RowBox[{"\[Pi]", " ", "and", " ", 
          RowBox[{"\[Pi]", "."}]}]}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"rgealpha", "=", 
       RowBox[{"NDSolve", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"ar", "'"}], "[", "q", "]"}], "==", 
            RowBox[{
             RowBox[{"-", "I"}], 
             RowBox[{"(", 
              RowBox[{
               RowBox[{"b0C", " ", 
                SuperscriptBox[
                 RowBox[{"ar", "[", "q", "]"}], "2"]}], "+", 
               RowBox[{"b1C", " ", 
                SuperscriptBox[
                 RowBox[{"ar", "[", "q", "]"}], "3"]}], "+", 
               RowBox[{"b2C", " ", 
                SuperscriptBox[
                 RowBox[{"ar", "[", "q", "]"}], "4"]}]}], ")"}]}]}], " ", ",", 
           RowBox[{
            RowBox[{"ar", "[", "0", "]"}], "\[Equal]", 
            RowBox[{"apwrexpand", "[", 
             RowBox[{"s0", ",", "aint", ",", "sint"}], "]"}]}]}], "}"}], ",", 
         "ar", ",", 
         RowBox[{"{", 
          RowBox[{"q", ",", 
           RowBox[{"-", "Pi"}], ",", "Pi"}], "}"}]}], "]"}]}], ";", " ", "\n", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "The", " ", "value", " ", "of", " ", "the", " ", "strong", " ", 
         "coupling", " ", "is", " ", "then", " ", "tabulated", " ", "at", " ",
          "discrete", " ", "points", " ", "around", " ", "the", " ", "circle",
          " ", "in", " ", "the", " ", "complex", " ", "s"}], "-", 
        RowBox[{"plane", "."}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"ralpha", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Evaluate", "[", 
          RowBox[{
           RowBox[{"ar", "[", "x", "]"}], "/.", "rgealpha"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "h", ",", "Pi", ",", "h"}], "}"}]}], "]"}]}], ";",
       "\n", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "Calculating", " ", "the", " ", "running", " ", "mass", " ", "at", 
         " ", "each", " ", "point", " ", "around", " ", "the", " ", 
         "circle"}], ",", " ", 
        RowBox[{
        "by", " ", "using", " ", "the", " ", "value", " ", "of", " ", "the", 
         " ", "coupling", " ", "\n", "constant", " ", "and", " ", "solving", 
         " ", "the", " ", 
         SubscriptBox["m", "q"], 
         RowBox[{"(", "s", ")"}], " ", "renormalization", " ", "group", " ", 
         "differential", " ", "equation", " ", "at", " ", "each", " ", 
         "discrete", " ", 
         RowBox[{"point", ".", " ", 
          RowBox[{"Note", ":", " ", 
           RowBox[{
            RowBox[{"The", " ", "list", " ", "of", " ", 
             SubscriptBox["\[Alpha]", "s"], " ", "at", " ", "each", " ", 
             "point", " ", "between"}], "-", 
            RowBox[{
            "\[Pi]", " ", "and", " ", "\[Pi]", " ", "is", " ", "given", " ", 
             "by", " ", 
             RowBox[{"ralpha", "."}]}]}]}]}]}]}], "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"rgemass", "=", 
       RowBox[{"Accumulate", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"h", "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"g0C", " ", "*", 
              RowBox[{"ralpha", "[", 
               RowBox[{"[", "k", "]"}], "]"}]}], " ", "+", 
             RowBox[{"g1C", " ", "*", 
              SuperscriptBox[
               RowBox[{"ralpha", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "2"]}], "+", 
             RowBox[{"g2C", " ", "*", 
              SuperscriptBox[
               RowBox[{"ralpha", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "3"]}], "+", 
             RowBox[{"g3C", " ", "*", 
              SuperscriptBox[
               RowBox[{"ralpha", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "4"]}]}], ")"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", "step"}], "}"}]}], "]"}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"rmass", "=", 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "I"}], "*", "rgemass"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "In", " ", "order", " ", "to", " ", "perform", " ", "the", " ", 
         "contour", " ", "integral"}], ",", " ", 
        RowBox[{
         RowBox[{
         "the", " ", "following", " ", "change", " ", "of", " ", "variables", 
          " ", "is", " ", 
          RowBox[{"made", ":", " ", "s"}]}], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"-", "s0"}], "*", 
           RowBox[{"Exp", "[", 
            RowBox[{"I", "*", "x"}], "]"}], " ", "and", " ", "ds"}], "=", 
          RowBox[{
           RowBox[{"-", "s0"}], "*", "I", "*", 
           RowBox[{"Exp", "[", 
            RowBox[{"I", "*", "x"}], "]"}], "*", "dx"}]}]}]}], "*)"}], "\n", 
      RowBox[{"contourintegral", "=", 
       RowBox[{"Total", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"h", "*", 
           RowBox[{"(", 
            RowBox[{"2", "*", 
             RowBox[{"Re", "[", 
              RowBox[{
               RowBox[{"(", 
                FractionBox[
                 RowBox[{"-", "1"}], 
                 RowBox[{"2", "*", "Pi", "*", "I"}]], ")"}], "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"-", "s0"}], "*", "I", "*", 
                 RowBox[{"Exp", "[", 
                  RowBox[{"I", "*", "k", "*", "h"}], "]"}]}], ")"}], "*", 
               RowBox[{"(", 
                RowBox[{
                 RowBox[{
                  RowBox[{"correlator", "/.", " ", 
                   RowBox[{"s", " ", "\[Rule]", " ", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"-", "s0"}], "*", 
                    RowBox[{"Exp", "[", 
                    RowBox[{"I", "*", "k", "*", "h"}], "]"}]}], ")"}]}]}], 
                  " ", "/.", " ", 
                  RowBox[{"ms0", " ", "\[Rule]", " ", 
                   RowBox[{"rmass", "[", 
                    RowBox[{"[", "k", "]"}], "]"}]}]}], "/.", " ", 
                 RowBox[{"a", " ", "\[Rule]", 
                  RowBox[{"ralpha", "[", 
                   RowBox[{"[", "k", "]"}], "]"}]}]}], ")"}], "*", 
               RowBox[{"(", 
                RowBox[{"kernelcontribution", "/.", 
                 RowBox[{"s", " ", "\[Rule]", 
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"-", "s0"}], "*", 
                    RowBox[{"Exp", "[", 
                    RowBox[{"I", "*", "k", "*", "h"}], "]"}]}], ")"}]}]}], 
                ")"}]}], "]"}]}], ")"}]}], "\[IndentingNewLine]", 
          "               ", ",", 
          RowBox[{"{", 
           RowBox[{"k", ",", "1", ",", "step"}], "}"}]}], "]"}], "]"}]}], ";",
       "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "contourintegral", "]"}]}]}], "]"}], 
   " "}]}]], "Input",
