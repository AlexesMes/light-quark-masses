Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"MassErrorFESR", "[", 
    RowBox[{
    "s0_", ",", "modD_", ",", "kernel_", ",", "MassFESR_", ",", 
     "correlator_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "quarkmass", ",", "\[CapitalDelta]as", ",", "\[CapitalDelta]s00", ",", 
       "\[CapitalDelta]s0", ",", "\[CapitalDelta]HAD", ",", 
       "\[CapitalDelta]GG", ",", "\[CapitalDelta]tot", ",", 
       "\[CapitalDelta]6LOOP", ",", " ", "massanderrors"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "mass", " ", "at", " ", "2", "GeV", " ", "with", " ", "standard", " ", 
       "input"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"quarkmass", "=", 
       RowBox[{"MassFESR", "[", 
        RowBox[{
        "s0", ",", "aint0", ",", "sint0", ",", "modD", ",", "kernel", ",", 
         "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon", ",", "1", ",", 
         "correlator"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "uncertainty", " ", "in", " ", "the", " ", "coupling", " ", 
        "constant"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]as", "=", 
       RowBox[{
        RowBox[{"MassFESR", "[", 
         RowBox[{"s0", ",", 
          RowBox[{"aint0", "+", "uncert0"}], ",", "sint0", ",", "modD", ",", 
          "kernel", ",", "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon", ",", 
          "1", ",", "correlator"}], "]"}], "-", "quarkmass"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "uncertainty", " ", "in", " ", "the", " ", "gluon", " ", 
        "condensate"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]GG", "=", 
       RowBox[{
        RowBox[{"MassFESR", "[", 
         RowBox[{
         "s0", ",", "aint0", ",", "sint0", ",", "modD", ",", "kernel", ",", 
          RowBox[{"GluonCon", "+", "uncertGluonCon"}], ",", "LQuarkCon", ",", 
          "SQuarkCon", ",", "1", ",", "correlator"}], "]"}], "-", 
        "quarkmass"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "uncertainty", " ", "due", " ", "to", " ", "the", " ", "range", " ", 
         "of", " ", "s0"}], " ", "-", " ", 
        RowBox[{
         RowBox[{"vary", " ", 
          SubscriptBox["s", "0"], " ", "by"}], " ", "\[PlusMinus]", 
         RowBox[{"0.5", " ", 
          SuperscriptBox["GeV", "2"], " ", "in", " ", "the", " ", "window", 
          " ", "of", " ", "stability"}]}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]s00", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"MassFESR", "[", 
          RowBox[{
          "i", ",", "aint0", ",", "sint0", ",", "modD", ",", "kernel", ",", 
           "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon", ",", "1", ",", 
           "correlator"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"s0", "-", "0.5"}], ",", 
           RowBox[{"s0", "+", "0.5"}], ",", "0.1"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]s0", "=", 
       RowBox[{"Abs", "[", 
        RowBox[{
         FractionBox["1", "2"], "*", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Max", "[", "\[CapitalDelta]s00", "]"}], "-", 
           RowBox[{"Min", "[", "\[CapitalDelta]s00", "]"}]}], ")"}]}], 
        "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "uncertainty", " ", "in", " ", "the", " ", "hadronic", " ", "model", 
         " ", "obtained", " ", "by", " ", "varying", " ", "the", " ", 
         "hadronic", " ", 
         RowBox[{"parameters", ":", " ", 
          RowBox[{
          "approximate", " ", "width", " ", "of", " ", "first", " ", 
           "resonance", " ", 
           RowBox[{"(", 
            RowBox[{"250", " ", "+", 
             RowBox[{
              RowBox[{"-", " ", "25"}], " ", "MeV"}]}], ")"}]}]}]}], ",", " ", 
        RowBox[{
        "approximate", " ", "width", " ", "of", " ", "second", " ", 
         "resonance", " ", 
         RowBox[{"(", 
          RowBox[{"250", " ", "+", 
           RowBox[{
            RowBox[{"-", " ", "25"}], " ", "MeV"}]}], ")"}], " ", "and", " ", 
         "the", " ", "parameter", " ", "\[Lambda]", " ", 
         RowBox[{"(", 
          RowBox[{"0.7", "-", "1.1"}], ")"}], " ", "within", " ", "the", " ", 
         "acceptable", " ", "ranges"}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]HAD", "=", 
       RowBox[{
        RowBox[{"MassFESR", "[", 
         RowBox[{"s0", ",", "aint0", ",", "sint0", ",", 
          RowBox[{"modelDPS", "[", 
           RowBox[{"s", ",", "1", ",", "225", ",", "225", ",", "0.7"}], "]"}],
           ",", "kernel", ",", "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon",
           ",", "1", ",", "correlator"}], "]"}], "-", 
        RowBox[{"MassFESR", "[", 
         RowBox[{"s0", ",", "aint0", ",", "sint0", ",", 
          RowBox[{"modelDPS", "[", 
           RowBox[{"s", ",", "1", ",", "275", ",", "275", ",", "1.1"}], "]"}],
           ",", "kernel", ",", "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon",
           ",", "1", ",", "correlator"}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "uncertainty", " ", "in", " ", "knowing", " ", "the", " ", "6", "th", 
        " ", "loop", " ", "of", " ", "the", " ", "PQCD", " ", "term"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]6LOOP", "=", 
       RowBox[{
        RowBox[{"MassFESR", "[", 
         RowBox[{
         "s0", ",", "aint0", ",", "sint0", ",", "modD", ",", "kernel", ",", 
          "GluonCon", ",", "LQuarkCon", ",", "SQuarkCon", ",", "2", ",", 
          "correlator"}], "]"}], "-", "quarkmass"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "the", " ", "total", " ", "uncertainty", " ", "addeed", " ", "in", " ",
         "quadrature"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]tot", "=", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         SuperscriptBox["\[CapitalDelta]as", "2"], "+", 
         SuperscriptBox["\[CapitalDelta]HAD", "2"], "+", 
         SuperscriptBox["\[CapitalDelta]s0", "2"], "+", 
         SuperscriptBox["\[CapitalDelta]GG", "2"], "+", 
         SuperscriptBox["\[CapitalDelta]6LOOP", "2"]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"massanderrors", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"quarkmass", ",", "\"\<Mass MeV\>\""}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
          "\[CapitalDelta]as", ",", "\"\<\[CapitalDelta]\[Alpha]\>\""}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalDelta]GG", ",", "\"\<\[CapitalDelta]GG\>\""}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalDelta]s0", ",", "\"\<\[CapitalDelta]s0\>\""}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"Min", "[", "\[CapitalDelta]s00", "]"}], ",", 
           RowBox[{"Max", "[", "\[CapitalDelta]s00", "]"}], ",", 
           "\"\<Min s0 and Max s0\>\""}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalDelta]HAD", ",", "\"\<Model Error\>\""}], "}"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{
          "\[CapitalDelta]6LOOP", ",", "\"\<Unknown 6-loop PQCD term\>\""}], 
          "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"\[CapitalDelta]tot", ",", "\"\<Total\>\""}], "}"}]}], 
        "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Return", "[", "massanderrors", "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}]], "Input",
