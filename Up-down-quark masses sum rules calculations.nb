          RowBox[{
           RowBox[{
            RowBox[{"ar", "'"}], "[", "q", "]"}], "==", 
           RowBox[{
            RowBox[{"-", "I"}], 
            RowBox[{"(", 
             RowBox[{
              RowBox[{"b0C", " ", 
