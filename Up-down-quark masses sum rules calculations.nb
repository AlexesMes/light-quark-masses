       "Calculating", " ", "the", " ", "running", " ", "mass", " ", "at", " ",
         "each", " ", "point", " ", "around", " ", "the", " ", "circle"}], 
       ",", " ", 
       RowBox[{
       "by", " ", "using", " ", "the", " ", "value", " ", "of", " ", "the", 
        " ", "coupling", " ", "\n", "constant", " ", "and", " ", "solving", 
        " ", "the", " ", 
        SubscriptBox["m", "q"], 
        RowBox[{"(", "s", ")"}], " ", "renormalization", " ", "group", " ", 
        "differential", " ", "equation", " ", "at", " ", "each", " ", 
        "discrete", " ", 
        RowBox[{"point", ".", " ", 
         RowBox[{"Note", ":", " ", 
          RowBox[{
           RowBox[{"The", " ", "list", " ", "of", " ", 
            SubscriptBox["\[Alpha]", "s"], " ", "at", " ", "each", " ", 
            "point", " ", "between"}], "-", 
           RowBox[{
           "\[Pi]", " ", "and", " ", "\[Pi]", " ", "is", " ", "given", " ", 
            "by", " ", 
            RowBox[{"ralpha", "."}]}]}]}]}]}]}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"rgemass", "=", 
      RowBox[{"Accumulate", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"h", "*", 
