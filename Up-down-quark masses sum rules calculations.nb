Cell[BoxData[
 RowBox[{
  RowBox[{"contourimproved", "[", 
   RowBox[{
   "kernelcontribution_", ",", "correlator_", ",", " ", "step_", ",", "aint_",
     ",", "sint_", ",", "s0_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Module", "[", 
